
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Contact{
  id String @id @map("_id") @default(auto()) @db.ObjectId
  name String
  phone Int
  email String
  subject String
  message String
}

model Inquiry{
  id String @id @map("_id") @default(auto()) @db.ObjectId
  name String
  place String
  age Int
  gmail String
  whatsappno Int
}
